{
  "name": "artists-finder",
  "version": "1.0.0",
  "description": "search engine that obtains a list of the best artists in a given country",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass src/sass/main.scss src/css/style.css -w",
    "watch:sass": "node-sass src/sass/main.scss dist/css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 version\" dist/css/style.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open"
  },
  "author": "Adrian",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.10.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
